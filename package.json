    "@bbva-web-components/bbva-form-input": "^1.1.12",
    "@bbva-web-components/bbva-foundations-icons": "^1.25.0",
    "@bbva-web-components/bbva-foundations-microillustrations": "^6.1.0",
    "@bbva-web-components/bbva-foundations-spinner": "^1.0.13",
    "@bbva-web-components/bbva-table-default": "^3.6.9",
    "@bbva-web-components/bbva-web-form-date": "^1.5.0",
    "@bbva-web-components/bbva-web-form-select": "^1.2.5",
    "@bbva-web-components/bbva-web-form-text": "^1.1.13",
    "@bbva-web-components/bbva-web-navigation-menu": "^1.3.3",
    "@bbva-web-components/bbva-web-tab-default": "^1.1.0",
    "@bbva-web-components/bbva-web-table-body": "^1.5.19",
    "@bbva-web-components/bbva-web-table-footer": "^1.0.16",
    "@bbva-web-components/bbva-web-table-header": "^1.1.13",
    "@gcp/fga": "1.*",
    "@gcp/fga-front-expressjs": ">=0.2.0  <1.0.0",
    "all": "^0.0.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "gts": "^5.0.1",
    "jest": "^29.5.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "<=0.24.8",
    "typescript": ">=4.3.0 <5.0.0-0"
  },
  "engines": {
    "node": "18.x.x"
  },
  "jestSonar": {
    "sonar56x": true,
    "reportPath": "coverage",
    "reportFile": "sonar-report.xml",
    "indent": 4
  }
}
