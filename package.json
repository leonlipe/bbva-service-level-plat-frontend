{
  "name": "fga-archetype-example",
  "version": "0.1.0",
  "private": true,
  "description": "An example with FGA Archetype app running on Google App Engine.",
  "author": "GCP Team",
  "scripts": {
    "check": "npm run lint",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "predev": "make build-front",
    "dev": "nodemon",
    "predocs": "npm run compile",
    "docs": "typedoc",
    "fix": "gts fix",
    "gcp-build": "npm i && npm run compile",
    "lint": "gts lint",
    "start": "node ./build/src/server.js",
    "pretest": "npm run compile",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@bbva-web-components/bbva-button-default": "^5.15.15",
    "@bbva-web-components/bbva-core-icon": "^1.5.7",
